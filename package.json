  "scripts": {
    "start-chrome": "start chrome --disable-web-security --user-data-dir=\"C:/ChromeDev\" http://localhost:3000",
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts --openssl-legacy-provider build",
    "start-iris": "cross-env HOST=irisdemo.com PORT=3000 DANGEROUSLY_DISABLE_HOST_CHECK=true react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-both": "concurrently \"npm start\" \"wait-on http://localhost:3000 && npm run start-chrome\"",
